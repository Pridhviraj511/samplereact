pipeline {
    agent any
    environment {
    
    }
    stages {
        stage(Download) {
            steps {
                git 'https://github.com/Pridhviraj511/samplereact.git'
                }
             }
        stage('Read JSON file') {
            steps {
                script {
                    def var = readJSON(file: './var.json')
                    echo ${var.NAME}
                }
            }
        }
    }
}
